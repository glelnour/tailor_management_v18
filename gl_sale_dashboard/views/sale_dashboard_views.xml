<odoo>
    <data>
        <record id="action_sale_dashboard" model="ir.actions.client">
            <field name="name">Dashboard</field>
            <field name="tag">sale_dashboard</field>
        </record>

        <menuitem name="Dashboard" id="sale_dashboard" parent="sale.sale_menu_root"
                  sequence="1"
                  groups="sales_team.group_sale_manager"
                  action="action_sale_dashboard"/>

        <record id="view_partner_form" model="ir.ui.view">
            <field name="name">res.partner.form.inherits.is_new_customer</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <field name="category_id" position="after">
                    <field name="is_new_customer"/>
                </field>
            </field>
        </record>
    </data>

</odoo>
